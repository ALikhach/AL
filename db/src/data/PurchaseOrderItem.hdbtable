COLUMN TABLE "Hana.db.data::PurchaseOrderItem" (
	
       PURCHASEORDERITEMID INTEGER  GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 200000000 INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL,
       MATERIALNUMBER      NVARCHAR(10),
	   QUANTITY            INTEGER,
	   PURCHASEORDERID     INTEGER,
	
PRIMARY KEY ("PURCHASEORDERITEMID")
	
)

UNLOAD PRIORITY 5 AUTO MERGE